{
  "name": "@snowflake-so/safe-sdk",
  "version": "1.0.9",
  "description": "Snowflake Safe SDK - Interact with multisig wallets on Snowflake Safe",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn rimraf dist && tsc -p src",
    "watch": "tsc -w -p src",
    "lint": "npx eslint --debug '**/*.{js,ts}'",
    "lint:fix": "npx eslint --fix '**/*.{js,ts}'",
    "prettier:fix": "prettier --config .prettierrc.js '**/*.{js,ts}' --write",
    "prettier:check": "prettier --config .prettierrc.js '**/*.{js,ts}' --check",
    "prepublish": "tsc -p src",
    "test": "jest"
  },
  "files": [
    "/dist"
  ],
  "keywords": [
    "Snowflake Safe",
    "solana",
    "multisig",
    "automation",
    "sdk",
    "cron"
  ],
  "pre-commit": {
    "run": [
      "prettier:check",
      "prettier:fix",
      "lint"
    ]
  },
  "author": "team@snowflake.so",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snowflake-so/snowflake-safe-sdk.git"
  },
  "license": "ISC",
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@metaplex-foundation/js": "^0.19.4",
    "@metaplex-foundation/mpl-core": "^1.2.0",
    "@metaplex-foundation/umi": "^1.1.1",
    "@metaplex-foundation/umi-eddsa-web3js": "^1.1.1",
    "@metaplex-foundation/umi-rpc-web3js": "^1.1.1",
    "@metaplex-foundation/umi-web3js-adapters": "^1.1.1",
    "@project-serum/anchor": "^0.24.2",
    "@snowflake-so/safe-sdk": "^1.0.9",
    "@snowflake-so/snowflake-sdk": "^1.0.12",
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.87.0",
    "bn.js": "^5.2.1",
    "bs58": "^6.0.0",
    "buffer-layout": "^1.2.2",
    "lodash": "^4.17.21",
    "rpc-websockets": "^7.0.0"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/jest": "^27.5.2",
    "@types/mocha": "^9.1.0",
    "@types/node-fetch": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "dotenv": "^16.0.0",
    "eslint": "^8.0.0",
    "jest": "^27.5.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.1.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "directories": {
    "test": "test"
  },
  "bugs": {
    "url": "https://github.com/snowflake-so/snowflake-safe-sdk/issues"
  },
  "homepage": "https://github.com/snowflake-so/snowflake-safe-sdk#readme"
}
